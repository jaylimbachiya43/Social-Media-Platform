<h1>Create Post</h1>

<%= form_with model: @post do |form| %>
<div >
  <%= form.label :title %><br>
  <%= form.text_field :title, placeholder: 'Enter title' %><br>
</div>
<div >
  <% form.label :user_id %><br>
  <% form.number_field :user_id  %><br>
</div>


<div>
  <%= form.submit 'submite'%>
</div>
<%end%>